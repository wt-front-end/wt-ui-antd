{
  "name": "@watone/wt-ui-antd",
  "version": "0.0.3",
  "author": "xkloveme",
  "private": false,
  "license": "MIT",
  "main": "dist/WTUI.common.js",
  "unpkg": "dist/WTUI.umd.min.js",
  "files": [
    "dist/*",
    "index.js"
  ],
  "keywords": [
    "vue",
    "vue-component",
    "components",
    "ui"
  ],
  "description": "a lightweight ui for vue2.x",
  "scripts": {
    "dev": "vue-cli-service serve --open ./examples/main.js",
    "build": "vue-cli-service build --target lib --name WTUI ./packages/index.js",
    "pre": "vuepress dev docs",
    "dev:docs": "vuepress dev docs",
    "build:docs": "vuepress build docs",
    "docs": "bash ./update-docs.sh",
    "commit": "git add -A && git-cz",
    "release:lib": "release-it",
    "release:docs": "npm run build:docs && gh-pages -d docs/.vuepress/dist",
    "publish:npm": "git add -A && npm run build && release-it",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "generate": "yarn all-contributors generate",
    "test": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wt-front-end/wt-ui-antd.git"
  },
  "bugs": {
    "url": "https://github.com/wt-front-end/wt-ui-antd/issues"
  },
  "homepage": "https://wt-front-end.github.io/wt-ui-antd/",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "packages/**/*.{vue,ts,tsx,js,jsx,json}": [
      "prettier --write",
      "npm run lint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}",
      "requireCleanWorkingDir": false
    }
  },
  "dependencies": {
    "dayjs": "^1.10.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@form-create/ant-design-vue": "^2.5.7",
    "@vue/cli-plugin-babel": "^3.0.3",
    "@vue/cli-plugin-eslint": "^3.0.3",
    "@vue/cli-plugin-unit-jest": "^3.8.0",
    "@vue/cli-service": "^3.0.3",
    "@vue/test-utils": "1.0.0-beta.29",
    "@vusion/md-vue-loader": "1.0.2",
    "all-contributors-cli": "^6.20.0",
    "ant-design-vue": "^1.7.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "chalk": "^2.4.2",
    "commitizen": "^4.0.4",
    "conventional-changelog-cli": "^2.0.31",
    "cz-conventional-changelog": "^3.1.0",
    "echarts": "^5.1.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "gh-pages": "^2.0.1",
    "highlight.js": "^9.15.8",
    "husky": "^1.2.1",
    "lint-staged": "^10.1.2",
    "material-design-icons": "^3.0.1",
    "nprogress": "^0.2.0",
    "popper.js": "^1.15.0",
    "prettier": "^1.15.3",
    "release-it": "^12.4.3",
    "sass-loader": "^11.1.1",
    "shelljs": "^0.8.3",
    "style-resources-loader": "^1.3.3",
    "vue": "2.6.13",
    "vue-jest": "^3.0.4",
    "vue-router": "^3.0.7",
    "vue-server-renderer": "2.6.13",
    "vue-template-compiler": "2.6.13",
    "vuepress": "^1.8.2",
    "vuepress-plugin-leo-demo-block": "0.1.1",
    "vuepress-theme-antdocs": "^1.3.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-unused-vars": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
